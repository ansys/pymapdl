SECDATA,0.5,1,90          ! LAYER 2: 0.5 THK, THETA 90
MP,EX,1,25E6
MP,EY,1,1E6
MP,EZ,1,25E6
MP,NUXY,1,.01
MP,NUYZ,1,.01
MP,NUXZ,1,.01
MP,GXY,1,.5E6
MP,GYZ,1,.2E6
MP,GXZ,1,.5E6
